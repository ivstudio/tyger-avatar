{
	"name": "tyger-avatar",
	"version": "1.0.10",
	"description": "Illustrated avatar react library",
	"homepage": "https://ivstudio.github.io/tyger-avatar-wiki/",
	"repository": {
		"type": "git",
		"url": "https://github.com/ivstudio/tyger-avatar"
	},
	"main": "dist/index.d.js",
	"module": "dist/index.esm.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rm -rf src/TrAvatars && rm -rf dist",
		"svgr": "svgr --icon --title-prop --no-dimensions --typescript -d src/TrAvatars assets",
		"build": "npm run clean && npm run svgr && rollup -c",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"keywords": [
		"Illustrated avatars",
		"React Avatar library",
		"Avatars"
	],
	"author": "Ignacio Villamar",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.15.0",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-node-resolve": "^13.0.4",
		"@storybook/addon-actions": "^6.3.7",
		"@storybook/addon-essentials": "^6.3.7",
		"@storybook/addon-links": "^6.3.7",
		"@storybook/react": "^6.3.7",
		"@svgr/cli": "^5.5.0",
		"@svgr/rollup": "^5.5.0",
		"@types/react": "^17.0.19",
		"babel-loader": "^8.2.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"rollup": "^2.56.2",
		"rollup-plugin-filesize": "^9.1.1",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-typescript2": "^0.30.0",
		"typescript": "^4.3.5"
	},
	"peerDependencies": {
		"react": "^16.8.0"
	},
	"dependencies": {}
}
